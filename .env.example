# OpenAI API Key - Required for text analysis
OPENAI_API_KEY=your_openai_api_key_here

# AssemblyAI API Key - Required for video/audio transcription
ASSEMBLYAI_API_KEY=your_assemblyai_api_key_here

# Upstash Redis - Required for queue management
# Get these from https://upstash.com
UPSTASH_REDIS_REST_URL=https://...upstash.io
UPSTASH_REDIS_REST_TOKEN=...

# Webhook Security - Generate a random secret
WEBHOOK_SECRET=your-secret-key-here

# Optional: Set your OpenAI model preference
# OPENAI_MODEL=gpt-4o